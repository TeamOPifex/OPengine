varying lowp vec2 vUV; uniform sampler2D uTexture; void main() {	lowp float d = 0.005;	lowp vec4 color = vec4(0.0);	int samples = 0;	for(int i = 1; i >= -1; i--)		for(int j = 1; j >= -1; j--){			lowp float x = float(j) - 1.0;			lowp float y = float(i) - 1.0;			lowp vec2 off = vec2(x * d, y * d);			color += texture2D(uTexture, vUV + off);			++samples;		}	color /= float(samples);	gl_FragColor = color;}